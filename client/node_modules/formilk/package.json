{
    "name": "formilk",
    "description": "Design system created with Atomico",
    "version": "0.15.1",
    "type": "module",
    "scripts": {
        "start": "vite --force",
        "build": "vite build --outDir docs",
        "exports": "exports  src/components.ts src/options.ts src/tokens.ts \"src/*/*.tsx\" --types --exports --main components",
        "prepublishOnly": "npm run exports"
    },
    "dependencies": {
        "@atomico/hooks": "^3.29.0",
        "@uppercod/parse": "^0.4.2",
        "atomico": "^1.45.0"
    },
    "devDependencies": {
        "@atomico/exports": "latest",
        "@atomico/markdown": "latest",
        "@uppercod/vite-meta-url": "latest",
        "@uppercod/vite-meta-url-load-css": "latest",
        "doc-elements": "^2.1.0",
        "typescript": "^4.5.4",
        "vite": "^2.6.13"
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false
    },
    "module": "./dist/components.js",
    "exports": {
        "./tabs": "./dist/tabs/tabs.js",
        "./text": "./dist/text/text.js",
        "./textarea": "./dist/textarea/textarea.js",
        "./theme": "./dist/theme/theme.js",
        "./input-counter": "./dist/input-counter/input-counter.js",
        "./input-file-preview": "./dist/input-file/input-file-preview.js",
        "./input-file": "./dist/input-file/input-file.js",
        "./label": "./dist/label/label.js",
        "./radio": "./dist/radio/radio.js",
        "./select-option": "./dist/select/select-option.js",
        "./select": "./dist/select/select.js",
        "./switch": "./dist/switch/switch.js",
        "./checkbox": "./dist/checkbox/checkbox.js",
        "./divide": "./dist/divide/divide.js",
        "./dropdown": "./dist/dropdown/dropdown.js",
        "./grid": "./dist/grid/grid.js",
        "./use-checkbox": "./dist/hooks/use-checkbox.js",
        "./icon": "./dist/icon/icon.js",
        "./icons": "./dist/icon/icons.js",
        "./input": "./dist/input/input.js",
        ".": "./dist/components.js",
        "./options": "./dist/options.js",
        "./tokens": "./dist/tokens.js",
        "./accordion-group": "./dist/accordion/accordion-group.js",
        "./accordion": "./dist/accordion/accordion.js",
        "./alert": "./dist/alert/alert.js",
        "./avatar": "./dist/avatar/avatar.js",
        "./button": "./dist/button/button.js"
    },
    "types": "types/components.d.ts",
    "typesVersions": {
        "*": {
            "options": [
                "./types/options.d.ts"
            ],
            "tokens": [
                "./types/tokens.d.ts"
            ],
            "theme": [
                "./types/theme/theme.d.ts"
            ],
            "grid": [
                "./types/grid/grid.d.ts"
            ],
            "icons": [
                "./types/icon/icons.d.ts"
            ],
            "icon": [
                "./types/icon/icon.d.ts"
            ],
            "divide": [
                "./types/divide/divide.d.ts"
            ],
            "input": [
                "./types/input/input.d.ts"
            ],
            "use-checkbox": [
                "./types/hooks/use-checkbox.d.ts"
            ],
            "checkbox": [
                "./types/checkbox/checkbox.d.ts"
            ],
            "switch": [
                "./types/switch/switch.d.ts"
            ],
            "radio": [
                "./types/radio/radio.d.ts"
            ],
            "select-option": [
                "./types/select/select-option.d.ts"
            ],
            "select": [
                "./types/select/select.d.ts"
            ],
            "button": [
                "./types/button/button.d.ts"
            ],
            "label": [
                "./types/label/label.d.ts"
            ],
            "alert": [
                "./types/alert/alert.d.ts"
            ],
            "avatar": [
                "./types/avatar/avatar.d.ts"
            ],
            "dropdown": [
                "./types/dropdown/dropdown.d.ts"
            ],
            "tabs": [
                "./types/tabs/tabs.d.ts"
            ],
            "text": [
                "./types/text/text.d.ts"
            ],
            "textarea": [
                "./types/textarea/textarea.d.ts"
            ],
            "accordion": [
                "./types/accordion/accordion.d.ts"
            ],
            "accordion-group": [
                "./types/accordion/accordion-group.d.ts"
            ],
            "input-file-preview": [
                "./types/input-file/input-file-preview.d.ts"
            ],
            "input-file": [
                "./types/input-file/input-file.d.ts"
            ],
            "input-counter": [
                "./types/input-counter/input-counter.d.ts"
            ],
            "components": [
                "./types/components.d.ts"
            ]
        }
    }
}